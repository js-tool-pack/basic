  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "main": "dist/basic.cjs.js",
  "module": "dist/basic.esm-bundler.js",
  "types": "dist/basic.d.ts",
  "exports": {
    ".": {
      "import": {
        "node": "./dist/basic.cjs.js",
        "default": "./dist/basic.esm-bundler.js"
      },
      "require": "./dist/basic.cjs.js"
    }
  },
  "buildOptions": {
    "name": "ToolPackBasic",
    "formats": [
      "esm-bundler",
      "esm-browser",
      "cjs",
      "global"
    ]
  },
  "publishConfig": {
