{
  "$schema": "https://typedoc.org/schema.json",
  "htmlLang": "zh-CN",
  "includeVersion": true,
  "plugin": ["@mxssfd/typedoc-theme"],
  "theme": "my-theme",
  "customCss": "./typedoc.theme.css",
  "sort": ["static-first", "kind", "enum-value-ascending"],
  "categoryOrder": ["file"],
  "out": "./docs-dist",
  "categorizeByGroup": true,
  "searchCategoryBoosts": {
    "Component": 2,
    "Model": 1.2
  },
  "searchGroupBoosts": {
    "Classes": 1.5
  },
  "navigationLinks": {
    "GitHub": "https://github.com/js-tool-pack/basic"
  },
  "entryPoints": ["src/index.typedoc.ts"]
}
